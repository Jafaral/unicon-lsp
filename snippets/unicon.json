{
	"Procedure Template": {
		"prefix": "pro",
		"body": [
			"#<p>",
			"#   ${4:Procedure Description}",
			"#   <[param ${5:arg1} ${6:description of first argument}]>",
			"#   <[param ${7:arg2} ${8:description of second argument}]>",
			"#   <[returns ${9:description of return value}]>",
			"#</p>",
			"procedure ${1:name}(${2:arg1}, ${3:arg2})",
			"\tlocal ${10:# variables}",
			"\t${0:# your code here}",
			"end",
			""
		],
		"description": "Create a procedure"
	},
	"Main Procedure": {
		"prefix": "mai",
		"body": [
			"#<p>",
			"#   ${3:Procedure Description}",
			"#   <[param ${4:arg1} ${5:description of first argument}]>",
			"#   <[param ${6:arg2} ${7:description of second argument}]>",
			"#   <[returns ${8:description of return value}]>",
			"#</p>",
			"procedure main(${1:arg1}, ${2:arg2})",
			"\tlocal ${9:# variables}",
			"\t${0:# your code here}",
			"end",
			""
		],
		"description": "Create a main procedure"
	},
	"Class Template": {
		"prefix": "cla",
		"body": [
			"#<p>",
			"#   ${4:Class Description}",
			"#</p>",
			"class ${1:className}(${2:var1}, ${3:var2})",
			"#<p>",
			"#   ${11:Method Description}",
			"#   <[param ${12:arg1} ${13:description of first argument}]>",
			"#   <[param ${14:arg2} ${15:description of second argument}]>",
			"#   <[returns ${16:description of return value}]>",
			"#</p>",
			"\tmethod ${8:methodName}(${9:arg1}, ${10:arg2})",
			"\t\tlocal ${17:# variables}",
			"\t\t${0:# your code here}",
			"\tend",
			"",
			"\tinitially(${5:arg1}, ${6:arg2})",
			"\t\t${7:# your code here}",
			"end",
			""
		],
		"description": "Create a class"
	},
	"Method Template": {
		"prefix": "met",
		"body": [
			"#<p>",
			"#   ${4:Method Description}",
			"#   <[param ${5:arg1} ${6:description of first argument}]>",
			"#   <[param ${7:arg2} ${8:description of second argument}]>",
			"#   <[returns ${9:description of return value}]>",
			"#</p>",
			"method ${1:name}(${2:arg1}, ${3:arg2})",
			"\tlocal ${10:# variables}",
			"\t${0:# your code here}",
			"end",
			""
		],
		"description": "Create a method for a class"
	},
	"Write Statement": {
		"prefix": "wre",
		"body": [
			"write(${0:message})"
		],
		"description": "Create a write statement"
	},
	"While Loop": {
		"prefix": "whi",
		"body": [
			"while ${1:condition} do {",
			"\t${0:# your code here}",
			"}"
		],
		"description": "Create a while loop"
	},
	"If Statement": {
		"prefix": "if",
		"body": [
			"if ${1:condition} then {",
			"\t${0:# your code here}",
			"}"
		],
		"description": "Create an if statement"
	},
	"If-Else Statement": {
		"prefix": "ife",
		"body": [
			"if ${1:condition} then {",
			"\t${2:# if true}",
			"} else {",
			"\t${3:# if false}",
			"}"
		],
		"description": "Create an if-else statement"
	},
	"Every Loop": {
		"prefix": "eve",
		"body": [
			"every ${1:var} := ${2:!list} do {",
			"\t${0:# loop body}",
			"}"
		],
		"description": "Create an every loop"
	},
	"Repeat Loop": {
		"prefix": "rep",
		"body": [
			"repeat {",
			"\t${1:# your code here}",
			"\tif ${2:condition} then break",
			"}"
		],
		"description": "Create a repeat loop"
	},
	"Case Statement": {
		"prefix": "cas",
		"body": [
			"case ${1:expression} of {",
			"\t${2:value1}: ${3:# code1}",
			"\t${4:value2}: ${5:# code2}",
			"\tdefault: ${6:# default code}",
			"}"
		],
		"description": "Create a case statement"
	},
	"Suspend Statement": {
		"prefix": "sus",
		"body": [
			"suspend ${1:expression1} do {",
			"\t${0:expression2}",
			"}"
		],
		"description": "Create a suspend statement"
	},
	"Until Statement": {
		"prefix": "unt",
		"body": [
			"until ${1:expression1} do {",
			"\t${0:expression2}",
			"}"
		],
		"description": "Create an until statement"
	},
	"critical Statement": {
		"prefix": "cri",
		"body": [
			"critical ${1:mutex} : ${0:expression}"
		],
		"description": "Create a critical statement"
	}
}